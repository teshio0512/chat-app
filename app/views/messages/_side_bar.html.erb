<div class="user_name">
  <div class="name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="chat_make">
    <%= link_to 'チャットを作成する', new_room_path, class: "room-make-btn" %>
  </div>
</div>
<div class="room_index">
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room-name">
        <a href="#"><%= room.name %></a>
      </div>
    </div>
  <% end %>
</div>